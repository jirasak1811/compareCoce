    body
        div

            // BEGIN BACK TO TOP
            a#totop(href='#')
                i.fa.fa-angle-up
            // END BACK TO TOP
            // BEGIN TOPBAR
            #header-topbar-option-demo.page-header-topbar
                nav#topbar.navbar.navbar-default.navbar-static-top(role='navigation' style='margin-bottom: 0; z-index: 2;')
                    .navbar-header
                        button.navbar-toggle(type='button' data-toggle='collapse' data-target='.sidebar-collapse')
                            span.sr-only Toggle navigation
                            span.icon-bar
                            span.icon-bar
                            span.icon-bar
                        a#logo.navbar-brand(href='/b2e/schedules')
                            span.fa.fa-rocket
                            span.logo-text TBS B2E
                            span.logo-text-icon(style='display: none') T
                    .topbar-main
                        .navbar-right.btn-group(style='float: left; height: 50px; padding: 10px 10px; font-size: 18px; line-height: 20px;')
                            button.btn.btn-default(type='button' ) Admin : 
                                =login_name
                            button.btn.btn-default.dropdown-toggle(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
                                span.caret
                                span.sr-only Toggle Dropdown
                            ul.dropdown-menu
                                li
                                    a(href='/b2e/users/logout' style="border-radius: 3px; color: #ffffff; background-color: #d9534f; border-color: #d74b47") Logout






            #wrapper
                // BEGIN SIDEBAR MENU
                nav#sidebar.navbar-default.navbar-static-side(role='navigation')
                    .sidebar-collapse.menu-scroll
                        ul#side-menu.nav
                            .clearfix
                            //- li#dashboard.active
                            //-     a(href='/dashboard')
                            //-         i.fa.fa-tachometer.fa-fw
