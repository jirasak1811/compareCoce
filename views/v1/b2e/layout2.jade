                            //-         span.menu-title Dashboard
                            li#schedules.active
                                a(href='/b2e/schedules')
                                    i.fa.fa-calendar.fa-fw
                                        .icon-bg.bg-orange
                                    span.menu-title Schedules
                            li#report
                                a(href='/b2e/report')
                                    i.fa.fa-file.fa-fw
                                        .icon-bg.bg-pink
                                    span.menu-title Report
                            li#email
                                a(href='/b2e/setting/email')
                                    i.fa.fa-envelope.fa-fw
                                        .icon-bg.bg-pink
                                    span.menu-title Email
                            li#smtp
                                a(href='/b2e/setting/smtp')
                                    i.fa.fa-gear.fa-fw
                                        .icon-bg.bg-pink
                                    span.menu-title SMTP

