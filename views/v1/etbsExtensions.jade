extend ../layout2
block content
  // Content Header (Page header)
  section.content-header
    h1(data-i18n='extensions')Extensions
  // Main content
  section.content
    .row
      .col-xs-12
        .box
          .box-header
            // h3.box-title Extension
          // /.box-header
          .box-body.table-responsive
            p
              .form-group
                label.control-label(for='name' , data-i18n='Search by name') Search by Name:
                input#name.form-control(name='name')
            table#datatable-extensions.table.table-bordered.table-striped
              thead
                tr
                  th(data-i18n='extension') Extension
                  th(data-i18n='Client ID') Client ID
                  th(data-i18n='Name') Name
                  th(data-i18n='Org ID') Org ID
                  th(data-i18n='Type') Type
                  th(data-i18n='License') License
                  th(data-i18n='Budget') Budget
                  th(data-i18n='Balance') Balance
                  th(data-i18n='Tenant') Tenant
                  th(data-i18n='Authorization code') Authorization code
                  th(data-i18n='Rent charge') Rent charge
              tbody
                each extension in extensions
                  tr
                    td= extension.extension
                    td= extension.clientid
                    td= extension.name
                    td= extension.orgid
                    td= extension.exttype
                    td= extension.has_license
                    td= extension.budget
                    td= extension.balance
                    td= extension.tenant
                    td= extension.authorization_code
                    td= extension.rent_charge
              tfoot
                tr
                  th(data-i18n='extension') Extension
                  th(data-i18n='Client ID') Client ID
                  th(data-i18n='Name') Name
                  th(data-i18n='Org ID') Org ID
                  th(data-i18n='Type') Type
                  th(data-i18n='License') License
                  th(data-i18n='Budget') Budget
                  th(data-i18n='Balance') Balance
                  th(data-i18n='Tenant') Tenant
                  th(data-i18n='Authorization code') Authorization code
                  th(data-i18n='Rent charge') Rent charge
          // /.box-body
        // /.box
  // /.content
